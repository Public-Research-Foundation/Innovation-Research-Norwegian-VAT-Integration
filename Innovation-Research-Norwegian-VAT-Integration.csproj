<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<LangVersion>latest</LangVersion>
		<Nullable>enable</Nullable>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<Title>Norsk Skattebibliotek</Title>
		<Description>Et .NET Standard bibliotek for norske skatteberegninger. Støtter personskatt, MVA, trygdeavgift og ENK-beregninger.</Description>
		<Version>1.0.0</Version>
		<Authors>Norsk Skattebibliotek Team</Authors>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageTags>skatt;skatteberegning;norge;mva;trygdeavgift;enk</PackageTags>
	</PropertyGroup>

	<ItemGroup>
	  <Folder Include="Mellomvare\" />
	</ItemGroup>


	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="7.0.0" />
		<PackageReference Include="Microsoft.Extensions.Logging" Version="7.0.0" />
		<PackageReference Include="Microsoft.Extensions.Options" Version="7.0.0" />
	</ItemGroup>
</Project>
